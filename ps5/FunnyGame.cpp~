#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main()
{
    int n = 0;
    while(true)
    {
        cin >> n;
        if(n == 0)
            break;

        if(n < 3)
            cout << "Alice" << endl;
        else
            cout << "Bob" << endl;
    }
    return 0;
}
